{
    "name": "johannesschobel/laravel-revisionable",
    "description": "Makes Laravel Models revisionable",
    "license": "MIT",
    "keywords": ["laravel", "eloquent", "revisionable", "revision", "history"],
    "authors": [
        {
            "name": "Johannes Schobel",
            "email": "johannes.schobel@googlemail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "illuminate/database": "5.4.*",
        "illuminate/support": "5.4.*",
        "illuminate/auth": "5.4.*",
        "cartalyst/sentry": "~2.0",
        "cartalyst/sentinel": "~2.0",
        "tymon/jwt-auth": "^0.5.3",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JohannesSchobel\\Revisionable\\RevisionableServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-0": {
            "JohannesSchobel\\Revisionable": "src/"
        }
    }
}
